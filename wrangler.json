{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "whip-demo",
	"main": "./src/worker/index.ts",
	"compatibility_date": "2025-04-01",
	"compatibility_flags": [
		"nodejs_compat"
	],
	"observability": {
		"enabled": true
	},
	"upload_source_maps": true,
	"kv_namespaces": [
	  {
		"binding": "KVASA",
