{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "cfstream",
	"main": "./src/worker/index.ts",
	"compatibility_date": "2025-04-01",
	"compatibility_flags": [
		"nodejs_compat"
	],
	"observability": {
		"enabled": true
	},
	"upload_source_maps": true,
	"kv_namespaces": [
	],
	"migrations": [
		{
			"new_sqlite_classes": [
				"LiveRoom",
				"Hbbs",
				"Hbbr",
			],
			"tag": "v1",
		},
	],
	"durable_objects": {
		"bindings": [
			{
				"class_name": "LiveRoom",
				"name": "LIVE_ROOM",
			},
			{
				"class_name": "Hbbs",
				"name": "HBBS",
			},
			{
				"class_name": "Hbbr",
				"name": "HBBR",
			},
		],
	},
	"vars": {
		"ENV": "dev"
	},
	"assets": {
		"directory": "./dist/client",
		"not_found_handling": "single-page-application"
	}
}
